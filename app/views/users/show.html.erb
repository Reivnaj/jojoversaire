<div class="card mt-3">
	<div class="card-header">
		<h4 class="card-title"> <%= @user.first_name %>  <%= @user.last_name %> </h4>
	</div>
	<div class="card-body">
		<p class="card-text"><%= @user.description %></p>
	</div>
	<div class="card-footer d-flex justify-content-between">
		<a class="card-link" href=#> Email : <%= @user.email %> </a>
    <%= link_to 'Changer mes infos perso', edit_user_registration_path, class: "card-link " %>
	</div>
</div>

<% @user.administarted_events.each do |event| %>
  <%= link_to event.title, event_path(event) %><br>
<%end%>
